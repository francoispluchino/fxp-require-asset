{
    "name": "fxp/require-asset",
    "type": "library",
    "description": "Helper of required assets for Twig with Assetic or Webpack",
    "keywords": ["asset", "require", "webpack", "twig", "assetic", "javascript", "stylesheet", "user interface", "ui"],
    "homepage": "https://github.com/fxpio/fxp-require-asset",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "symfony/filesystem": "^3.0.0",
        "symfony/finder": "^3.0.0",
        "symfony/config": "^3.0.0"
    },
    "require-dev": {
        "twig/twig": "^2.0.0",
        "kriswallsmith/assetic": "^1.3.0",
        "psr/cache": "^1.0.0"
    },
    "suggest": {
        "fxp/composer-asset-plugin": "For manage NPM/Bower dependencies with Composer"
    },
    "autoload": {
        "psr-4": { "Fxp\\Component\\RequireAsset\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
