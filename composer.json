{
    "name": "fxp/require-asset",
    "type": "library",
    "description": "Helper of required assets for Twig with Webpack",
    "keywords": ["asset", "require", "webpack", "twig", "javascript", "stylesheet", "user interface", "ui"],
    "homepage": "https://github.com/fxpio/fxp-require-asset",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/filesystem": "^4.1.0",
        "symfony/finder": "^4.1.0",
        "symfony/config": "^4.1.0"
    },
    "require-dev": {
        "twig/twig": "^2.4.0",
        "psr/cache": "^1.0.0"
    },
    "suggest": {
        "foxy/foxy": "To inject the NPM dependencies of PHP libraries in project NPM dependencies",
        "fxp/composer-asset-plugin": "To manage NPM/Bower dependencies with Composer"
    },
    "autoload": {
        "psr-4": { "Fxp\\Component\\RequireAsset\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
